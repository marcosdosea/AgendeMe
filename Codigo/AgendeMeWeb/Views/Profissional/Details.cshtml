@model AgendeMeWeb.Models.ProfissionalViewModel

@{
    ViewData["Title"] = "Detalhes";
}

<h1>Detalhes</h1>

<div>
    <h4>@Model.NomePrefeitura</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NomeProfissional)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomeProfissional)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NomePrefeitura)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomePrefeitura)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NomeCargo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomeCargo)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { IdProfissional = Model.IdProfissional, IdCargo = Model.IdCargo, IdPrefeitura = Model.IdPrefeitura }) |
    <a asp-action="Index">Voltar para a lista</a>
</div>
